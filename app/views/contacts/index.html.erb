<h1>My contacts</h1>
<a href='<%=get_login_url%>'>Log in and view my email</a>
<table>
  <tr>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Email</th>
    <th>Birthday</th>
    <th>JobTitle</th>
    <th>CompanyName</th>
    <th>MobilePhone</th>
    <th>CompanyPhone</th>
  </tr>
  <% !@contacts.blank? && @contacts.each do |contact| %>
    <tr>
      <td><%= contact['GivenName'] %></td>
      <td><%= contact['Surname'] %></td>
      <td><%= contact['EmailAddresses'][0]['Address'] rescue "" %></td>
      <td><%= contact['Birthday'] %></td>
      <td><%= contact['JobTitle'] %></td>
      <td><%= contact['CompanyName'] %></td>
      <td><%= contact['MobilePhone1'] %></td>
      <td><%= contact['BusinessPhones'].blank? ? "" : contact['BusinessPhones'].first%></td>
    </tr>
  <% end %>
</table>